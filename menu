#include<stdio.h>
#include<stdlib.h>
#include"header.h"

void menu()
{
    printf("\n     ======================================\n");
   int i;
    printf("\n");
    printf("\n\t1. All profiles.\n");
    printf("\t2. Input.\n");
    printf("\t3. Search.\n\n");
    printf("\tEnter your choice: ");
    scanf("%d", &i);


    if(i==3) searchinfo();
    if(i==2) inputinfo();
    if(i==1) outputinfo();

}

